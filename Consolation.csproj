<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.0.0.0</Version>
    <Nullable>enable</Nullable>
    <Authors>Tomat</Authors>
    <Description>Command line interface library</Description>
    <Copyright>MIT License</Copyright>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/Steviegt6/Consolation</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Steviegt6/Consolation</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>cli, command line, console</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
